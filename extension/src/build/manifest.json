{
  "name": "Keepies",
  "description": "A memento of your app over time",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "tabs", "downloads", "storage", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "ui.html"
  }
}
